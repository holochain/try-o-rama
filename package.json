{
  "name": "@holochain/tryorama",
  "description": "E2E test framework for Holochain apps",
  "version": "0.5.0",
  "author": "Holochain Foundation",
  "repository": {
    "type": "git",
    "url": "https://github.com/holochain/tryorama"
  },
  "license": "CAL-1.0",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "exports": {
    ".": "./lib/*.js"
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "rimraf ./lib && tsc -p tsconfig.build.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "lint": "eslint --fix --ext .ts src test .eslintrc.js",
    "test": "node --experimental-specifier-resolution=node --loader ts-node/esm ts/test/index.ts",
    "test:unit": "ts-node test/unit",
    "test:e2e": "ts-node test/e2e"
  },
  "dependencies": {
    "@holochain/client": "^0.3.2",
    "@iarna/toml": "^2.2.5",
    "async-mutex": "^0.1.4",
    "base-64": "^0.1.0",
    "colors": "^1.4.0",
    "fp-ts": "^2.8.5",
    "get-port": "^5.1.1",
    "io-ts": "^2.2.12",
    "io-ts-reporters": "^1.2.2",
    "lodash": "^4.17.20",
    "memoizee": "^0.4.14",
    "ramda": "^0.26.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "ws": "^8.5.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@msgpack/msgpack": "^2.7.2",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.20.45",
    "@types/ramda": "^0.26.44",
    "@types/tape": "^4.13.2",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sinon": "^7.5.0",
    "tape": "^5.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
